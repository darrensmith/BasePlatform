        "passport-http-bearer": "1.0.1",
        "passport-oauth2-client-password": "0.1.2",
        "passport-custom": "^1.0.5",
        "oauth2orize": "^1.2.0",
        "socket.io": "^1.3.7",
        "strict-rate-limiter": "^0.1.1",
        "swagger-ui": "^2.1.3",
        "orm": "^2.1.29",
        "mysql": "^2.10.0",
        "connect-flash": "^0.1.1",
        "express-session": "^1.12.1",
        "uid2": "^0.0.3",
        "request": "^2.62.0",
        "http-proxy": "1.13.2",
